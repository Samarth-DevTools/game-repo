{
    "name": "tetris",
    "version": "0.5.5",
    "description": "Simple html5 canvas tetris game",
    "devDependencies": {
        "http-server": "^14.1.1",
        "jest": "^30.0.5",
        "nyc": "^17.1.0"
    },
    "scripts": {
        "test": "jest",
        "start": "http-server",
        "coverage": "nyc --reporter=lcov --reporter=text npm test"
    },
    "nyc": {
        "all": true,
        "include": ["src/tetris.js"],
        "reporter": ["text", "lcov"],
        "exclude": ["tests", "node_modules"]
    }
}      
